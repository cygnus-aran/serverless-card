{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://ksh/Merchant",
  "title": "Merchant",
  "properties": {
    "private_id": {
      "type": "string",
      "pattern": "^[0-9]{20,}$"
    },
    "public_id": {
      "type": "string",
      "pattern": "^[0-9]{20,}$"
    },
    "enable": {
      "type": "boolean"
    }
  },
  "required": ["private_id", "public_id", "enable"],
  "type": "object"
}
