{
  "name": "@kushki/usrv-card",
  "version": "1.0.0",
  "description": "usrv-card",
  "main": "handler.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint:duplicate && npm run lint:check && npm run test:unit"
    }
  },
  "scripts": {
    "deploy": "serverless deploy -v",
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check",
    "lint:check": "npx eslint --ext=ts src",
    "lint:duplicate": "npx jscpd ./src",
    "lint:fix": "npx eslint --fix --ext=ts src",
    "lint:format": "prettier --write \"./**/*.{ts,js,json,yml,yaml}\" \"./.{pull_review.kushkirc,lintstagedrc,nycrc,prettierrc}\"",
    "package": "serverless package",
    "sonar": "nyc report --reporter=lcov",
    "test:coverage": "npx nyc mocha -config ./.mocharc.nyc.yml src/**/*.spec.ts",
    "test": "npm run test:unit && npm run test:coverage",
    "test:integration": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "npx mocha --watch src/**/*.spec.ts",
    "test:unit": "npx mocha src/**/*.spec.ts",
    "tsc:check": "tsc --project tsconfig.check.json --noEmit",
    "tsc:interface": "kushki json2ts",
    "validate": "npm run depcheck && npm list && npm run lint && npm run test",
    "husky-prepare": "husky install",
    "depcheck": "depcheck . --ignores=\"serverless-*,@types/*,@kushki/*,prettier,@sendgrid/helpers,aws-lambda,country-to-iso,aws-xray-sdk-core,acorn,tsutils,rollbar,chai,husky,lint-staged,ajv,tslint-config-prettier,infrastructure,constant,middleware,core,gateway,service,error,moment-timezone,dot-object,moment,lodash,faker,camelcase-keys,change-case,nano-seconds,string-similarity,uuid,repository,types,parse-full-name,deep-cleaner,snake-case,snakecase-keys,aws-sdk,aws-xray-sdk,axios,inversify,inversify-logger-middleware,jsonwebtoken,middy,reflect-metadata,rxjs,rxjs-spy,chai-json-schema,sinon,sinon-chai,ts-mockery,jsonpath,microtime-node,esbuild,@aws-sdk/client-dynamodb, @aws-sdk/client-sqs, @aws-sdk/client-ssm, @aws-sdk/client-s3,@aws-sdk/client-sns, @aws-sdk/client-eventbridge, @aws-sdk/lib-dynamodb, aws-sdk-client-mock,@aws-lambda-powertools/tracer,@middy/core,node-rsa,fs-extra,mockdate\""
  },
